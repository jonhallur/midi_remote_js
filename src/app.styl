@import "./loader.css"

.float-right {
  float: right;
};

.midi-control-label {

  height: 25px;
  border-bottom: 1px solid #ddd;
  background-color: #f5f5f5;
  border-top-left-radius: 3px;
  border-top-right-radius: 3px;
  text-align: center;

}

.midi-control-label p {
  font-size: 11px;
  padding: 5px;
};

.midi-control-box {
  float: left;
  width: 80px;
  height 102px;
  border: 1px solid #ddd;
  border-radius: 5px;
  margin: 5px;
};

.dial-box {
  margin: 9px;
  width: 60px;
  height 60px;
}
